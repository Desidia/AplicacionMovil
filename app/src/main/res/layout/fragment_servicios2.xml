<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.ii.is.aplicacion.aplicacionmovil.ServiciosFragment"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <!-- TODO: Update blank fragment layout -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#F3F3F3"
        tools:layout_editor_absoluteX="-3dp"
        tools:layout_editor_absoluteY="-4dp">

        <ListView
            android:id="@+id/Lista"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/Crear"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="12dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:divider="@drawable/divider"
            tools:layout_editor_absoluteX="-768dp"
            tools:layout_editor_absoluteY="209dp" />

        <Button
            android:id="@+id/Lugares"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView7"
            android:backgroundTint="#CBF0FE"
            android:text="Mis lugares"
            android:textColor="#42CAFD"
            android:textSize="24dp"
            android:layout_centerHorizontal="true"
            tools:layout_editor_absoluteX="39dp"
            tools:layout_editor_absoluteY="16dp" />

        <Button
            android:id="@+id/Crear"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/Lugares"
            android:layout_alignStart="@+id/Lugares"
            android:layout_below="@+id/Lugares"
            android:backgroundTint="#CBF0FE"
            android:text="AÃ‘ADIR LUGAR   +"
            android:textColor="#42CAFD"
            android:textSize="24dp"
            tools:layout_editor_absoluteX="198dp"
            tools:layout_editor_absoluteY="16dp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="18dp"
            android:text="MIS LUGARES"
            android:textColor="#42CAFD"
            android:textSize="30sp"
            android:textStyle="bold"
            android:layout_alignBottom="@+id/imageView2"
            android:layout_alignEnd="@+id/Lugares" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="22dp"
            android:visibility="visible"
            app:srcCompat="@drawable/imagen_mis_lugares"
            android:layout_alignParentTop="true"
            android:layout_alignStart="@+id/Lugares" />
    </RelativeLayout>
    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="37dp"
        android:layout_height="37dp"
        app:srcCompat="@drawable/finger_mislugares"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />
</FrameLayout>
