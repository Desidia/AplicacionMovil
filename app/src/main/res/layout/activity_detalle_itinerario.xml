<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.ii.is.aplicacion.aplicacionmovil.DetalleItinerario">

    <RelativeLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">

        <TextView
            android:id="@+id/titutloitinerario"
            android:layout_width="200dp"
            android:layout_height="40dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="24dp"
            android:text="TextView" />

        <TextView
            android:id="@+id/tex"
            android:layout_width="80dp"
            android:layout_height="35dp"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/titutloitinerario"
            android:layout_marginStart="23dp"
            android:layout_marginTop="13dp"
            android:text="Creador" />

        <TextView
            android:id="@+id/creadoritinerario"
            android:layout_width="190dp"
            android:layout_height="35dp"
            android:layout_alignBaseline="@+id/tex"
            android:layout_alignBottom="@+id/tex"
            android:layout_marginStart="22dp"
            android:layout_toEndOf="@+id/tex"
            android:text="TextView" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="95dp"
            android:layout_height="35dp"
            android:layout_alignStart="@+id/tex"
            android:layout_below="@+id/tex"
            android:layout_marginTop="17dp"
            android:text="Disponibilidad" />

        <TextView
            android:id="@+id/disponibilidaditinerario"
            android:layout_width="190dp"
            android:layout_height="35dp"
            android:layout_alignBaseline="@+id/textView5"
            android:layout_alignBottom="@+id/textView5"
            android:layout_alignStart="@+id/creadoritinerario"
            android:text="TextView" />

        <ListView
            android:id="@+id/viewitinerario"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/disponibilidaditinerario"
            android:layout_marginStart="12dp"
            android:layout_marginTop="19dp" />
    </RelativeLayout>
</android.support.constraint.ConstraintLayout>
